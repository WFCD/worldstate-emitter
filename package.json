{
  "name": "worldstate-emitter",
  "version": "0.0.0-dev",
  "description": "Event emitter for worldstate & other warframe events",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha test/specs --exit",
    "lint": "eslint index.js handlers/**.js utilities/**.js",
    "lint:fix": "eslint index.js handlers/**.js utilities/**.js --fix",
    "coverage": "npm test && nyc report --reporter=text-lcov | npx coveralls",
    "build-docs": "npx jsdoc -c jsdoc-config.json -d docs",
    "dev": "npx nodemon test/tester.js",
    "snyk-protect": "npx snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "directories": {
    "test": "test/specs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wfcd/worldstate-emitter.git"
  },
  "keywords": [
    "warframe",
    "worldstate",
    "event",
    "emitter"
  ],
  "author": "tobiah <tobiah@protonmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wfcd/worldstate-emitter/issues"
  },
  "homepage": "https://github.com/wfcd/worldstate-emitter#readme",
  "dependencies": {
    "@sentry/node": "^5.21.4",
    "colors": "^1.4.0",
    "json-fetch-cache": "^1.2.6",
    "rss-feed-emitter": "^3.2.2",
    "twitter": "^1.7.1",
    "warframe-worldstate-data": "^1.8.1",
    "warframe-worldstate-parser": "^2.14.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "ink-docstrap": "^1.3.2",
    "mocha": "^7.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "nodemonConfig": {
    "delay": 5000,
    "env": {
      "LOG_LEVEL": "debug",
      "CACHE_TIMEOUT": 60000,
      "SEMLAR_TIMEOUT": 300000
    }
  },
  "snyk": true
}
