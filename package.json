{
  "name": "worldstate-emitter",
  "version": "0.0.0-dev",
  "description": "Event emitter for worldstate & other warframe events",
  "main": "index.js",
  "scripts": {
    "test": "npx nyc --reporter=text npx mocha test/specs --exit",
    "lint": "npx eslint index.js",
    "lint:fix": "npx eslint index.js --fix",
    "coverage": "npm test && npx nyc report --reporter=text-lcov | npx coveralls",
    "build-docs": "npx jsdoc -c jsdoc-config.json -d docs",
    "dev": "npx nodemon test/tester.js"
  },
  "directories": {
    "test": "test/specs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wfcd/worldstate-emitter.git"
  },
  "keywords": [
    "warframe",
    "worldstate",
    "event",
    "emitter"
  ],
  "author": "tobiah <tobiah@protonmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wfcd/worldstate-emitter/issues"
  },
  "homepage": "https://github.com/wfcd/worldstate-emitter#readme",
  "dependencies": {
    "@sentry/node": "^5.7.1",
    "colors": "^1.4.0",
    "json-fetch-cache": "^1.2.2",
    "rss-feed-emitter": "github:filipedeschamps/rss-feed-emitter#0c6beac",
    "twitter": "^1.7.1",
    "warframe-worldstate-data": "^1.3.3",
    "warframe-worldstate-parser": "^2.9.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "ink-docstrap": "^1.3.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "nodemonConfig": {
    "delay": 5000,
    "env": {
      "LOG_LEVEL": "debug",
      "CACHE_TIMEOUT": 60000,
      "SEMLAR_TIMEOUT": 300000
    }
  }
}
